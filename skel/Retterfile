configure api_revision: <%= Retter::API_REVISION %> do |config|
  config.url            = 'http://retter.example.com/'
  config.title          = '<%= name %>'
  config.description    = '<%= name %>'
  config.author         = '<%= ENV["USER"] %>'

  config.publisher do
    # system 'git push origin master'
  end
end
